<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    
    <body>
        <referenceBlock name="page.main.title" remove="true" />
		
		<referenceContainer name="main.content">
			<container name="home.register.div" htmlClass="login_div back_white_titlw" htmlTag="section" after="columns">
				<container name="home.register.div.row" htmlClass="row" htmlTag="div" before="-">
					<container name="home.register.div.row.col-12" htmlClass="col-12" htmlTag="div" before="-">
						<container name="home.register.div.row.col-12.login_inner" htmlClass="login_inner" htmlTag="div" before="-">
							<container name="home.register.div.row.col-12.login_inner.row" htmlClass="row" htmlTag="div" before="-">
							</container>							
						</container>
					</container>
				</container>
			</container>
		</referenceContainer>
		
		<move element="customer_form_register" destination="home.register.div.row.col-12.login_inner.row" before="-" />
		
    </body>
</page>
